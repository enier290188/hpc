msgid "HPC___SSH___MESSAGES_ClusterNotAvailable"
msgstr "Imposible establecer una conexión con el cluster en este momento."
msgid "HPC___SSH___MESSAGES_AuthenticationException"
msgstr "Autentificación fallida, verifique sus credenciales."
msgid "HPC___SSH___MESSAGES_BadHostKeyException"
msgstr "No se puede verificar la clave de host del servidor."
msgid "HPC___SSH___MESSAGES_SSHException"
msgstr "No se puede establecer una conexión SSH con el cluster."
msgid "HPC___SSH___MESSAGES_Exception"
msgstr "En estos momentos nuestro cluster HPC está inactivo debido a labores de mantenimiento, intente el acceso posteriormente."
msgid "HPC___SSH___MESSAGES_BadConnection"
msgstr "No se puede establecer la conexión. El clúster está deshabilitado en este momento."
msgid "HPC___SSH___MESSAGES_CommandError"
msgstr "Error de comando: %s"
msgid "HPC___SSH___MESSAGES_OperationException"
msgstr "Error de operación."
msgid "HPC___SSH___MESSAGES_UploadFilesException"
msgstr "No ha sido posible subir el archivo \"%s\" al servidor."
msgid "HPC___SSH___MESSAGES_DownloadFilesException"
msgstr "No ha sido posible descargar el archivo \"%s\" del servidor."
msgid "HPC___SSH___MESSAGES_WriteIsNotPossible"
msgstr "Se ha producido un error al intentar modificar el archivo."
msgid "HPC___SSH___MESSAGES_OpenIsNotPossible"
msgstr "Se ha producido un error al intentar abrir el archivo."

msgid "HPC___SSH___MESSAGES_ScriptFormErr"
msgstr "El formulario contiene errores, verifique cada campo."
msgid "HPC___SSH___MESSAGES_ScriptSendOk"
msgstr "La secuencia de comandos se ha guardado en su carpeta de usuario."
msgid "HPC___SSH___MESSAGES_ScriptSendErr"
msgstr "Se produjo un error al guardar la secuencia de comandos en su carpeta de usuario."
msgid "HPC___SSH___MESSAGES_ScriptReleasedOk"
msgstr "Secuencia de comandos guardada y lanzada. (%s)"
msgid "HPC___SSH___MESSAGES_ScriptReleasedErr"
msgstr "Secuencia de comandos guardada. Error en la ejecución. (%s)"

msgid "HPC___SSH___MESSAGES_OpenFileNotEncodingText"
msgstr "Solo se pueden editar archivos en formato texto. (%s)"