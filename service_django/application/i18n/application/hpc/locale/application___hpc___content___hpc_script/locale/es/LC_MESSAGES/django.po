msgid "HPC___CONTENT___SCRIPT___MODAL___VARIABLES___TABLE___SLURM_TASKS_PER_NODE"
msgstr "Número de tareas que se iniciarán en cada nodo."
msgid "HPC___CONTENT___SCRIPT___MODAL___VARIABLES___TABLE___SLURM_TASK_PID"
msgstr "El ID de proceso de la tarea que se inicia."
msgid "HPC___CONTENT___SCRIPT___MODAL___VARIABLES___TABLE___SLURMD_NODENAME"
msgstr "Nombre del nodo que ejecuta el script de trabajo."
msgid "HPC___CONTENT___SCRIPT___MODAL___VARIABLES___URL"
msgstr "Para mas información visite el sitio web <a>https://slurm.schedmd.com/sbatch.html</a>"