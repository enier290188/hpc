# *****************************************************************************
# APPLICATION HPC CONTENT HPC_SCRIPT FORM FIELDS
# *****************************************************************************

msgid "HPC___CONTENT___SCRIPT___FIELD___SCRIPT_NAME___TITLE"
msgstr "Script name"
msgid "HPC___CONTENT___SCRIPT___FIELD___SCRIPT_NAME___PLACEHOLDER"
msgstr "- Insert script name -"
msgid "HPC___CONTENT___SCRIPT___FIELD___SCRIPT_NAME___POPOVER___TITLE"
msgstr "Script name"
msgid "HPC___CONTENT___SCRIPT___FIELD___SCRIPT_NAME___POPOVER___CONTENT"
msgstr "In this field you can insert a name for your script. Remember that if there is already a script with the same name in your home folder, it'll be overwritten. The extension &quot;.sl&quot; is added automatically."

msgid "HPC___CONTENT___SCRIPT___FIELD___JOB_NAME___TITLE"
msgstr "Job name"
msgid "HPC___CONTENT___SCRIPT___FIELD___JOB_NAME___PLACEHOLDER"
msgstr "- Insert job name -"
msgid "HPC___CONTENT___SCRIPT___FIELD___JOB_NAME___POPOVER___TITLE"
msgstr "Job name"
msgid "HPC___CONTENT___SCRIPT___FIELD___JOB_NAME___POPOVER___CONTENT"
msgstr "<b>--job-name=&quot;jobname&quot;</b><br>Specify a name for the job allocation. The specified name will appear along with the job id number when querying running jobs on the system. The default is the name of the batch script."

msgid "HPC___CONTENT___SCRIPT___FIELD___ENV_VARS___TITLE"
msgstr "Export variables"
msgid "HPC___CONTENT___SCRIPT___FIELD___ENV_VARS___POPOVER___TITLE"
msgstr "Export environment variables"
msgid "HPC___CONTENT___SCRIPT___FIELD___ENV_VARS___POPOVER___CONTENT"
msgstr "<b>--export=&quot;environment variables [ALL] | NONE&quot;</b><br>Identify which environment variables are propagated to the launched application, by default all are propagated."

msgid "HPC___CONTENT___SCRIPT___FIELD___PARTITION___TITLE"
msgstr "Partition"
msgid "HPC___CONTENT___SCRIPT___FIELD___PARTITION___POPOVER___TITLE"
msgstr "Partition selection"
msgid "HPC___CONTENT___SCRIPT___FIELD___PARTITION___POPOVER___CONTENT"
msgstr "<b>--partition=&quot;partition_names&quot;</b><br>Request a specific partition for the resource allocation. If not specified, the default behavior is to allow the slurm controller to select the default partition as designated by the system administrator."

msgid "HPC___CONTENT___SCRIPT___FIELD___RESOURCE_LIST_NODES___TITLE"
msgstr "Resource list"
msgid "HPC___CONTENT___SCRIPT___FIELD___RESOURCE_LIST_NODES___PLACEHOLDER"
msgstr "- Nodes count -"
msgid "HPC___CONTENT___SCRIPT___FIELD___RESOURCE_LIST_NODES___PLACEHOLDER_2"
msgstr "- Total tasks (across all nodes) -"
msgid "HPC___CONTENT___SCRIPT___FIELD___RESOURCE_LIST_NODES___POPOVER___TITLE"
msgstr "Resource list (Nodes)"
msgid "HPC___CONTENT___SCRIPT___FIELD___RESOURCE_LIST_NODES___POPOVER___CONTENT"
msgstr "<b>--nodes=&quot;minnodes[-maxnodes]&quot;</b><br>Request that a minimum of nodes (minnodes) be allocated to this job. A maximum node count may also be specified (maxnodes). If only one number is specified, this is used as both the minimum and maximum node count.<br><b>--ntasks=&quot;number&quot;</b><br>sbatch does not launch tasks, it requests an allocation of resources and submits a batch script. This option advises the Slurm controller that job steps run within the allocation will launch a maximum of number tasks and to provide for sufficient resources."

msgid "HPC___CONTENT___SCRIPT___FIELD___RESOURCE_LIST_PROCESSES___TITLE"
msgstr "Resource list"
msgid "HPC___CONTENT___SCRIPT___FIELD___RESOURCE_LIST_PROCESSES___PLACEHOLDER"
msgstr "- Tasks per nodes -"
msgid "HPC___CONTENT___SCRIPT___FIELD___RESOURCE_LIST_PROCESSES___PLACEHOLDER_2"
msgstr "- CPU cores per tasks -"
msgid "HPC___CONTENT___SCRIPT___FIELD___RESOURCE_LIST_PROCESSES___POPOVER___TITLE"
msgstr "Resource list (tasks)"
msgid "HPC___CONTENT___SCRIPT___FIELD___RESOURCE_LIST_PROCESSES___POPOVER___CONTENT"
msgstr "<b>--tasks-per-node=&quot;n&quot;</b><br>Specify the number of tasks to be launched per node.<br><b>--cpus-per-task=&quot;ncpus&quot;</b><br>Advise the Slurm controller that ensuing job steps will require ncpus number of processors per task. Without this option, the controller will just try to allocate one processor per task."

msgid "HPC___CONTENT___SCRIPT___FIELD___MEMORY___TITLE"
msgstr "Memory"
msgid "HPC___CONTENT___SCRIPT___FIELD___MEMORY___PLACEHOLDER"
msgstr "- RAM per node -"
msgid "HPC___CONTENT___SCRIPT___FIELD___MEMORY___PLACEHOLDER_2"
msgstr "- RAM per CPU core -"
msgid "HPC___CONTENT___SCRIPT___FIELD___MEMORY___POPOVER___TITLE"
msgstr "Memory"
msgid "HPC___CONTENT___SCRIPT___FIELD___MEMORY___POPOVER___CONTENT"
msgstr "<b>--mem=&quot;size[units]&quot;</b><br>Specify the real memory required per node. Default units are megabytes. Different units can be specified using the suffix [K|M|G|T]<br><b>--mem-per-cpu=&quot;size[units]&quot;</b><br>Minimum memory required per allocated CPU. Default units are megabytes."

msgid "HPC___CONTENT___SCRIPT___FIELD___WALLTIME___TITLE"
msgstr "Walltime"
msgid "HPC___CONTENT___SCRIPT___FIELD___WALLTIME___PLACEHOLDER"
msgstr "- Wall clock limit (mm o [[dd-]hh:]mm:ss o dd-hh[:mm[:ss]]) -"
msgid "HPC___CONTENT___SCRIPT___FIELD___WALLTIME___POPOVER___TITLE"
msgstr "Walltime"
msgid "HPC___CONTENT___SCRIPT___FIELD___WALLTIME___POPOVER___CONTENT"
msgstr "<b>--time=&quot;time&quot;</b><br>Set a limit on the total run time of the job allocation. If the requested time limit exceeds the partition's time limit, the job will be left in a PENDING state (possibly indefinitely). The default time limit is the partition's default time limit."

msgid "HPC___CONTENT___SCRIPT___FIELD___TEST___TITLE"
msgstr "Test"
msgid "HPC___CONTENT___SCRIPT___FIELD___TEST___POPOVER___TITLE"
msgstr "Test"
msgid "HPC___CONTENT___SCRIPT___FIELD___TEST___POPOVER___CONTENT"
msgstr "<b>--test-only</b><br>Validate the batch script and return an estimate of when a job would be scheduled to run given the current job queue and all the other arguments specifying the job requirements. No job is actually submitted."

msgid "HPC___CONTENT___SCRIPT___FIELD___GPU___TITLE"
msgstr "Require GPUs"
msgid "HPC___CONTENT___SCRIPT___FIELD___GPU___POPOVER___TITLE"
msgstr "Require GPUs"
msgid "HPC___CONTENT___SCRIPT___FIELD___GPU___POPOVER___CONTENT"
msgstr "<b>--gres=&quot;list&quot;</b><br>Specifies a comma delimited list of generic consumable resources. The format of each entry on the list is &quot;name[[:type]:count]&quot;.The name is that of the consumable resource. The count is the number of those resources with a default value of 1. The specified resources will be allocated to the job on each node. The available generic consumable resources is configurable by the system administrator. Examples of use include <b>--gres=gpu:2,mic=1</b>, <b>--gres=gpu:kepler:2</b>, and <b>--gres=help</b>."

msgid "HPC___CONTENT___SCRIPT___FIELD___NODELIST___TITLE"
msgstr "Nodelist"
msgid "HPC___CONTENT___SCRIPT___FIELD___NODELIST___PLACEHOLDER"
msgstr "- Preferred nodes -"
msgid "HPC___CONTENT___SCRIPT___FIELD___NODELIST___POPOVER___TITLE"
msgstr "List of preferred nodes"
msgid "HPC___CONTENT___SCRIPT___FIELD___NODELIST___POPOVER___CONTENT"
msgstr "<b>--nodelist=&quot;node name list&quot;</b><br>Request a specific list of hosts. The job will contain all of these hosts and possibly additional hosts as needed to satisfy resource requirements. The list may be specified as a comma-separated list of hosts, a range of hosts (host[1-5,7,...] for example), or a filename. The host list will be assumed to be a filename if it contains a &quot;/&quot; character."

msgid "HPC___CONTENT___SCRIPT___FIELD___EXCLUDE___TITLE"
msgstr "Exclude"
msgid "HPC___CONTENT___SCRIPT___FIELD___EXCLUDE___PLACEHOLDER"
msgstr "- Nodes to avoid -"
msgid "HPC___CONTENT___SCRIPT___FIELD___EXCLUDE___POPOVER___TITLE"
msgstr "List of nodes to avoid"
msgid "HPC___CONTENT___SCRIPT___FIELD___EXCLUDE___POPOVER___CONTENT"
msgstr "<b>--exclude=&quot;node name list&quot;</b><br>Explicitly exclude certain nodes from the resources granted to the job."

msgid "HPC___CONTENT___SCRIPT___FIELD___OUTPUT_STREAM___TITLE"
msgstr "Output stream"
msgid "HPC___CONTENT___SCRIPT___FIELD___OUTPUT_STREAM___PLACEHOLDER"
msgstr "- Standard output file -"
msgid "HPC___CONTENT___SCRIPT___FIELD___OUTPUT_STREAM___POPOVER___TITLE"
msgstr "Output stream"
msgid "HPC___CONTENT___SCRIPT___FIELD___OUTPUT_STREAM___POPOVER___CONTENT"
msgstr "<b>--output=&quot;filename pattern&quot;</b><br>Instruct Slurm to connect the batch script's standard output directly to the file name specified in the &quot;filename pattern&quot;."

msgid "HPC___CONTENT___SCRIPT___FIELD___ERROR_STREAM___TITLE"
msgstr "Error stream"
msgid "HPC___CONTENT___SCRIPT___FIELD___ERROR_STREAM___PLACEHOLDER"
msgstr "- Standard error file -"
msgid "HPC___CONTENT___SCRIPT___FIELD___ERROR_STREAM___POPOVER___TITLE"
msgstr "Error stream"
msgid "HPC___CONTENT___SCRIPT___FIELD___ERROR_STREAM___POPOVER___CONTENT"
msgstr "<b>--error=&quot;filename pattern&quot;</b><br>Instruct Slurm to connect the batch script's standard error directly to the file name specified in the &quot;filename pattern&quot;."

msgid "HPC___CONTENT___SCRIPT___FIELD___MAIL_ADDRESS___TITLE"
msgstr "Mail address"
msgid "HPC___CONTENT___SCRIPT___FIELD___MAIL_ADDRESS___PLACEHOLDER"
msgstr "- Insert mail addresses, comma separeted -"
msgid "HPC___CONTENT___SCRIPT___FIELD___MAIL_ADDRESS___POPOVER___TITLE"
msgstr "Mail address"
msgid "HPC___CONTENT___SCRIPT___FIELD___MAIL_ADDRESS___POPOVER___CONTENT"
msgstr "<b>--mail-user=&quot;user&quot;</b><br>User to receive email notification of state changes as defined by <b>--mail-type.</b>"

msgid "HPC___CONTENT___SCRIPT___FIELD___MAIL_EVENT___TITLE"
msgstr "Mail on event"
msgid "HPC___CONTENT___SCRIPT___FIELD___MAIL_EVENT___CHECKBOX_1"
msgstr "Begin"
msgid "HPC___CONTENT___SCRIPT___FIELD___MAIL_EVENT___CHECKBOX_2"
msgstr "End"
msgid "HPC___CONTENT___SCRIPT___FIELD___MAIL_EVENT___CHECKBOX_3"
msgstr "Fail"
msgid "HPC___CONTENT___SCRIPT___FIELD___MAIL_EVENT___CHECKBOX_4"
msgstr "Requeue"
msgid "HPC___CONTENT___SCRIPT___FIELD___MAIL_EVENT___POPOVER___TITLE"
msgstr "Send emails when job ..."
msgid "HPC___CONTENT___SCRIPT___FIELD___MAIL_EVENT___POPOVER___CONTENT"
msgstr "<b>--mail-type=&quot;event&quot;</b><br>Notify user by email when certain event types occur. Valid type values are NONE, BEGIN, END, FAIL, REQUEUE, ALL."

msgid "HPC___CONTENT___SCRIPT___FIELD___BODY_SCRIPT___PLACEHOLDER"
msgstr "- Insert body of the script here... -"

